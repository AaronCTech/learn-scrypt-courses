import "util.scrypt";
contract MyHelloWorld {
    public function unlock(SigHashPreimage sighashPreimage, int amount) {
        require(Tx.checkPreimage(sighashPreimage));
        bytes scriptCode = Util.scriptCode(sighashPreimage);
        bytes output = Util.buildOutput(scriptCode, amount);

        require(hash256(output) == Util.hashOutputs(sighashPreimage));
    }
}
